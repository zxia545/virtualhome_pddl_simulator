(walk_towards character coffe_maker)
(walk_towards character coffee_filter)
(grab character coffee_filter)
(walk_towards character coffe_maker)
(put_on character coffee_filter coffe_maker)
(open_this character coffe_maker)
(walk_towards character ground_coffee)
(grab character ground_coffee)
(walk_towards character coffe_maker)
(close character coffe_maker)
(switch_on character coffe_maker)
(put_on character ground_coffee coffe_maker)
; cost = 12 (unit cost)
